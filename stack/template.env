# Template .env for stack/docker-compose.yml
# Copy this file to .env and update values before running `docker compose up -d`

# Top level domain
DOMAIN_NAME=example.com

# Subdomain for n8n (host will be SUBDOMAIN.DOMAIN_NAME)
SUBDOMAIN=n8n

# ACME/LetsEncrypt e-mail
ACME_EMAIL=admin@example.com

# Timezone for containers
TZ=Europe/Berlin

# Some images expect both TZ and GENERIC_TIMEZONE
GENERIC_TIMEZONE=${TZ}

# Optional: other variables referenced by compose
# e.g. PGVECTOR and other services may require secrets in ../secrets
