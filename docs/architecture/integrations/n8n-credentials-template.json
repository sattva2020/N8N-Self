{
  "credentials": [
    {
      "name": "Miro Credential",
      "type": "httpHeaderAuth",
      "data": {
        "name": "Authorization",
        "value": "Bearer {{MIRO_TOKEN}}"
      },
      "nodesAccess": {}
    },
    {
      "name": "Postgres (project-db)",
      "type": "postgres",
      "data": {
        "host": "{{PG_HOST}}",
        "port": "{{PG_PORT}}",
        "database": "{{PG_DATABASE}}",
        "user": "{{PG_USER}}",
        "password": "{{PG_PASSWORD}}",
        "ssl": false
      },
      "nodesAccess": {}
    }
  ]
}
