{
  "name": "dashboard-frontend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
  "dev": "vite",
  "build": "vite build",
  "preview": "vite preview",
  "serve-dist": "node ./scripts/simple-serve.js",
  "test": "vitest",
  "test:watch": "vitest --watch",
  "e2e": "playwright test",
  "e2e:headed": "playwright test --headed",
  "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,css,md}\"",
  "lint": "eslint \"src/**/*.{ts,tsx,js,jsx}\" --max-warnings=0",
  "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
  "axios": "^1.4.0"
  },
  "devDependencies": {
  "typescript": "^5.1.6",
  "vite": "^4.3.9",
  "@vitejs/plugin-react": "^4.0.0",
  "vitest": "^0.34.0",
  "@testing-library/react": "^14.0.0",
  "@testing-library/jest-dom": "^6.0.0",
  "jsdom": "^21.1.0",
  "@playwright/test": "^1.42.0",
  "tailwindcss": "3.4.4",
  "postcss": "8.4.24",
  "autoprefixer": "10.4.14",
  "minimist": "^1.2.8"
  }

  ,
  "lint-staged": {
    "src/**/*.{js,ts,tsx,jsx}": [
      "eslint --fix",
      "prettier --write",
      "git add"
    ],
    "**/*.{css,md,json}": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "es5",
    "printWidth": 100
  }
}
